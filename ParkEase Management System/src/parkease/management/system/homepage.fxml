<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="464.0" prefWidth="802.0" stylesheets="@../../home.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="parkease.management.system.HomepageController">
   <children>
      <ImageView fitHeight="472.0" fitWidth="898.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../homepage.png" />
         </image>
      </ImageView>
      <TableView fx:id="tableview" layoutX="9.0" layoutY="103.0" prefHeight="363.0" prefWidth="657.0" stylesheets="@../../table.css">
        <columns>
          <TableColumn fx:id="colID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="colType" prefWidth="95.0" text="Vehicle Type" />
            <TableColumn fx:id="colNum" prefWidth="115.0" text="Vehicle Number" />
            <TableColumn fx:id="colSlot" prefWidth="104.0" text="Allocated Slot" />
            <TableColumn fx:id="colErT" prefWidth="154.0" text="Entry Time" />
            <TableColumn fx:id="colAt" prefWidth="113.0" text="Allocated Time" />
        </columns>
      </TableView>
      <TextField fx:id="tfVN" layoutX="530.0" layoutY="46.0" prefHeight="48.0" prefWidth="132.0" promptText="Vehicle No." styleClass="modern-textfield" stylesheets="@../../txtfd.css" />
      <TextField fx:id="tfAT" layoutX="668.0" layoutY="46.0" prefHeight="48.0" prefWidth="158.0" promptText="Allocated Time" styleClass="modern-textfield" stylesheets="@../../txtfd.css" />
      <Button fx:id="addbtn" layoutX="669.0" layoutY="103.0" mnemonicParsing="false" onAction="#addActn" prefHeight="38.0" prefWidth="158.0" styleClass="transparent-button" text="Add">
         <stylesheets>
            <URL value="@../../style.css" />
            <URL value="@../../btnnew.css" />
         </stylesheets>
         <font>
            <Font name="Arial" size="11.0" />
         </font></Button>
      <Button fx:id="upbtn" layoutX="670.0" layoutY="204.0" mnemonicParsing="false" onAction="#upActn" prefHeight="38.0" prefWidth="158.0" styleClass="transparent-button" text="Update">
         <stylesheets>
            <URL value="@../../style.css" />
            <URL value="@../../btnnew.css" />
         </stylesheets></Button>
      <Button fx:id="delbtn" layoutX="669.0" layoutY="155.0" mnemonicParsing="false" onAction="#delActn" prefHeight="38.0" prefWidth="158.0" styleClass="transparent-button" text="Checkout">
         <stylesheets>
            <URL value="@../../style.css" />
            <URL value="@../../btnnew.css" />
         </stylesheets></Button>
      <Button fx:id="trnscbtn" layoutX="669.0" layoutY="253.0" mnemonicParsing="false" onAction="#transactn" prefHeight="38.0" prefWidth="158.0" styleClass="transparent-button" text="Transactions">
         <stylesheets>
            <URL value="@../../style.css" />
            <URL value="@../../btnnew.css" />
         </stylesheets></Button>
      <ComboBox fx:id="cbVehicleType" layoutX="185.0" layoutY="43.0" prefHeight="54.0" prefWidth="165.0" promptText="Vehicle type" styleClass="combo-box" stylesheets="@../../combobox.css" />
      <ComboBox fx:id="cbMetroCode" layoutX="359.0" layoutY="43.0" prefHeight="54.0" prefWidth="165.0" promptText="Metro Code" styleClass="combo-box" stylesheets="@../../combobox.css" />
      <ComboBox fx:id="cbAllocatedSlot" layoutX="9.0" layoutY="43.0" prefHeight="54.0" prefWidth="165.0" promptText="Parking Slot" styleClass="combo-box" stylesheets="@../../combobox.css" />
      <Button fx:id="logout" layoutX="708.0" layoutY="403.0" mnemonicParsing="false" onAction="#UserLogout" stylesheets="@../../btnnew.css" text="logout" />
      <Label fx:id="currentUser" layoutX="122.0" layoutY="5.0" prefHeight="31.0" prefWidth="110.0" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
